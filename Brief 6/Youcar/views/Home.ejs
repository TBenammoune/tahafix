  <body>
    <header>
      <div class="navbar-class">
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
          <a class="navbar-brand" href="#">YouCar</a>
          <button
            class="navbar-toggler"
            type="button"
            data-toggle="collapse"
            data-target="#navbarNavAltMarkup"
            aria-controls="navbarNavAltMarkup"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
              <a class="nav-item nav-link active" href="/"
                >Acceuil<span class="sr-only">(current)</span></a
              >
              <a class="nav-item nav-link" href="#">Notre voitures</a>
              <a class="nav-item nav-link" href="#">RÃ©servetion</a>
              <a class="nav-item nav-link" href="#">Contactez-nouz</a>
              <!-- <a class="nav-item nav-link disabled" href="#">Disabled</a> -->
            </div>
          </div>
        </nav>
      </div>
      <div class="header-class" >
        <h1>YouCar <span>location de voitures</span></h1>
      </div>
    </header>

      <h1 style="text-align: center;"> Ajouter Voiture </h1>
      <br />

      <div class="form-ajt-voiture" style="width: 50%; margin: 3px auto; text-align: center;">
        <div class="form-ajt-voiture">
          <form action="/" method="post">
            <div class="form-input">
              <input
                  style="display: none;"
                  type="text"
                  name="id"
                  class="form-control"
                  id="validationServer02"
                  value="<%=json.length+1%>"
                  placeholder="Nom de la voiture"
                  required
                />
                <br>
                <input
                  type="text"
                  name="name"
                  class="form-control"
                  id="validationServer02"
                  placeholder="Nom de la voiture"
                  required
                />
                <br>
                <input
                  type="text"
                  name="carburant"
                  class="form-control"
                  id="validationServer02"
                  placeholder="Genre de carburant"
                  required
                />
                <br>
                <input
                  type="text"
                  name="climatisation"
                  class="form-control"
                  id="validationServer02"
                  placeholder="La climatisation disponible?"
                  required
                />
                <br>
                <input
                  type="text"
                  name="price"
                  class="form-control"
                  id="validationServer02"
                  placeholder="Enter le prix de location"
                  required
                />
                <br>
                <button class="btn btn-success" type="submit">Ajouter la voiture</button>
            </div>
          </form>
      </div>
    <br />
    <footer class="footer">
